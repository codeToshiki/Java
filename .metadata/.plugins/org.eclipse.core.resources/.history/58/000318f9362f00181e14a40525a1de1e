package basic;

import basic.inheritance.SubInheritanceLearning;
import basic.inheritance.SuperInheritanceLearning;

public class ExecMain {

	public static void main(String[] args) {
		// 親クラスへのアップキャスト宣言の検証
		// 親クラス = 子クラス
		SuperInheritanceLearning sil = new SubInheritanceLearning();
		sil.method1();
		sil.method2();
	}

}
